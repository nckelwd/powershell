$date = (Get-Date).ToString('MM-dd-yyyy')
$ou =  "OU=APR,DC=aprenergy,DC=local"
$properties = "Enabled", "employeeID", "GivenName", "surname", "EmailAddress", "Department", "Title", "samAccountName", "whenCreated", "employeeType", "info"
$filter = "(Enabled -eq 'false')"
$users = Get-ADUser -Filter $filter -SearchBase $ou -SearchScope Subtree -Properties $properties |  select $properties

#loop through users
foreach ($u in $users) {

    Write-Host  $u.EmailAddress

}

#"msDS-cloudExtensionAttribute2"